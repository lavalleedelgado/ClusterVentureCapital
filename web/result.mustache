<!DOCTYPE html PUBLIC "-//IETF//DTD HTML 2.0//EN">
<html>
  <head>
    <title>Result</title>
    <link type='text/css' rel='stylesheet' href='austere.css' />
  </head>
  <body>
    <h3>{{msa_label}}</h3>
    <table>
      <tr>
        <th>SEC cluster</th>
        <th colspan='2'>Employment (thousands)</th>
        <th colspan='3'>Venture capital (thousands)</th>
      </tr>
      {{#clusters}}
      <tr>
        <td style='text-align: left;'>{{cluster_label}}</td>
        <td style='padding-right: 2px'>{{cluster_emp}}</td>
        <td style='padding-left: 2px'>{{cluster_emp_pct}}</td>
        <td style='padding-right: 0px'>$</td>
        <td style='padding-right: 2px'>{{cluster_amt}}</td>
        <td style='padding-left: 2px'>{{cluster_amt_pct}}</td>
      </tr>
      {{/clusters}}
    </table>
  </body>
</html>
