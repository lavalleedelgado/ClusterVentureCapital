<!DOCTYPE html PUBLIC "-//IETF//DTD HTML 2.0//EN">
<html>
  <head>
    <title>Result</title>
    <link type='text/css' rel='stylesheet' href='austere.css' />
  </head>
  <body>
    <h3>Venture capital in {{msa_label}} by industry cluster</h3>
    <table>
      <tr>
        <th>Cluster</th>
        <th>Employment</th>
        <th>Share of MSA total</th>
        <th>Venture capital</th>
        <th>Share of MSA total</th>
      </tr>
      {{#clusters}}
      <tr>
        <td>{{cluster_label}}</td>
        <td>{{cluster_emp}}</td>
        <td>{{cluster_emp_pct}}</td>
        <td>{{cluster_amt}}</td>
        <td>{{cluster_amt_pct}}</td>
      </tr>
      {{/clusters}}
    </table>
  </body>
</html>
